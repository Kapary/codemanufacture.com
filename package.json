{
  "name": "codemanufacture-website",
  "description": "Codemanufacture Website",
  "version": "0.1.0",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacure.com>",
  "private": true,
  "dependencies": {
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.6",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-google-tagmanager": "^1.0.18",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-robots-txt": "^1.0.2",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-typescript": "^1.4.20",
    "gatsby-source-filesystem": "^1.5.38",
    "gatsby-transformer-remark": "^1.7.41",
    "graphql-code-generator": "^0.9.1",
    "graphql-codegen-typescript-template": "^0.9.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "ts-loader": "^3.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": "^8.4.0"
  },
  "homepage": "https://codemanufacture.com/",
  "keywords": [
    "gatsby"
  ],
  "license": "Apache-2.0",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn lint:source && yarn nit:markdown && yarn nit:source",
    "develop": "gatsby develop -H 0.0.0.0",
    "format:markdown": "prettier --config .prettierrc --parser markdown --write '{./content/**/*.md,./README.md}'",
    "format:source": "prettier --config .prettierrc --parser typescript --write 'src/**/*.{ts,tsx}'",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts",
    "lint:source": "tslint './src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --config .prettierrc --parser markdown --list-different '{./content/**/*.md,./README.md}'",
    "nit:source": "prettier --config .prettierrc --parser typescript --list-different 'src/**/*.{ts,tsx}'",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "1.12.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0"
  }
}
